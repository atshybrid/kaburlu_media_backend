{
  "name": "kaburlu_media_backend",
  "description": "Kaburlu News Platform API (Node + Express + TypeScript)",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run start:dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start:dev:env": "dotenv -e .env.development -- ts-node-dev --respawn --transpile-only src/index.ts",
    "start:prod:env": "dotenv -e env.production -- node dist/index.js",
    "seed": "ts-node prisma/seed.ts",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:migrate:dev:env": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:migrate:deploy:prod:env": "dotenv -e env.production -- npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npm run seed",
    "seed:dev:env": "dotenv -e .env.development -- ts-node prisma/seed.ts",
    "seed:prod:env": "dotenv -e env.production -- node dist/prisma/seed.js",
    "seed:reporter-designations": "ts-node scripts/seed_reporter_designations.ts",
    "migrate:designations:global": "ts-node scripts/migrate_designations_to_global.ts",
    "seed:minimal": "ts-node scripts/seed_minimal.ts",
    "seed:demo-prashna": "ts-node scripts/seed_demo_prashna.ts",
    "seed:adilabad-mandals": "ts-node scripts/seed_adilabad_mandals.ts",
    "seed:adilabad-full": "ts-node scripts/seed_adilabad_full_mandals.ts",
    "reset:adilabad-mandals": "ts-node scripts/reset_adilabad_mandals.ts",
    "clear:mandals": "ts-node scripts/clear_mandals.ts",
    "seed:family": "echo 'family seeding removed'",
    "seed:kin": "ts-node scripts/seed_kin_relations.ts",
    "seed:surnames": "ts-node scripts/seed_surnames.ts",
    "seed:castes": "ts-node scripts/seed_castes.ts",
    "seed:ai-rewrite-prompts": "ts-node scripts/seed_ai_rewrite_prompts.ts",
    "clear:relations": "echo 'chat relations script removed'",
    "clear:tenant": "ts-node scripts/clear_tenant.ts",
    "ai:smoke": "ts-node scripts/aiDraftSmoke.ts",
    "jobs:ai-queue": "ts-node src/jobs/aiQueue.worker.ts",
    "jobs:ai-cron": "ts-node src/jobs/aiQueue.cron.ts",
    "jobs:ai-queue:prod": "node dist/jobs/aiQueue.worker.js",
    "jobs:ai-cron:prod": "node dist/jobs/aiQueue.cron.js",
    "jobs:location-populate": "ts-node src/workers/locationPopulateCron.ts",
    "jobs:location-populate:prod": "node dist/workers/locationPopulateCron.js",
    "jobs:db-backup": "ts-node src/jobs/dbBackup.cron.ts",
    "jobs:db-backup-cron": "ts-node src/jobs/dbBackup.cron.ts --cron",
    "jobs:db-backup:prod": "node dist/jobs/dbBackup.cron.js",
    "jobs:db-backup-cron:prod": "node dist/jobs/dbBackup.cron.js --cron",
    "backfill:web-category": "ts-node scripts/backfill_webarticle_category.ts",
    "backfill:web-category:dry": "ts-node scripts/backfill_webarticle_category.ts --dry-run",
    "prod:build": "npm run build",
    "prod:pm2:start": "npm run prod:build && npx pm2 start ecosystem.config.cjs",
    "prod:pm2:restart": "npx pm2 restart ecosystem.config.cjs",
    "prod:pm2:stop": "npx pm2 stop ecosystem.config.cjs",
    "prod:pm2:delete": "npx pm2 delete ecosystem.config.cjs",
    "prod:pm2:logs": "npx pm2 logs",
    "prompts:web": "ts-node scripts/update_web_prompt.ts",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": "20.x"
  },
  "author": "Kaburlu Media",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.2",
    "@sparticuz/chromium": "^143.0.4",
    "@types/cors": "^2.8.19",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.17.4",
    "@types/qrcode": "^1.5.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "firebase-admin": "^13.5.0",
    "fluent-ffmpeg": "^2.1.3",
    "google-auth-library": "^10.3.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "puppeteer": "^23.11.1",
    "qrcode": "^1.5.4",
    "razorpay": "^2.9.6",
    "react-native-razorpay": "^2.3.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "transliteration": "^2.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/helmet": "^4.0.0",
    "@types/node": "^20.19.13",
    "@types/transliteration": "^1.6.1",
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.0",
    "prisma": "^6.16.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
